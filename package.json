{
  "name": "roodle",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.20",
    "@mantine/dates": "^6.0.20",
    "@mantine/ds": "^6.0.20",
    "@mantine/hooks": "^6.0.20",
    "@mantine/prism": "^6.0.20",
    "@tabler/icons-react": "^2.33.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dayjs": "^1.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.2.2",
    "web-vitals": "^3.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cli-test": "npm test -- --bail=1 --noStackTrace --no-watchman --watchAll=false --runInBand",
    "test:ci": "CI=true npm run cli-test",
    "eject": "react-scripts eject",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "yarn lint-files .",
    "lint-files": "eslint --ext .ts,.tsx,.js,.tsx --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "prettierConfig": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "jsxSingleQuote": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@datadog/browser-rum": "^4.48.2",
    "clipboardy": "^3.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "msw": "^1.3.0",
    "prettier": "^3.0.3",
    "react-avatar": "^5.0.3",
    "react-datetime-picker": "^5.5.1",
    "react-router-dom": "^6.15.0",
    "sort-by": "^1.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint-files"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
